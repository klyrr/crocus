{
  "name": "krokus",
  "version": "0.6.0",
  "description": "Provider for localization patterns and a number and currency formatter.",
  "keywords": [
    "localization",
    "l10n",
    "format",
    "numberformat",
    "numberFormat",
    "currencyformat",
    "currencyFormat",
    "locale"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/klyrr/krokus.git"
  },
  "license": "MIT",
  "scripts": {
    "compile": "babel --optional runtime -d dist/js src/js",
    "test": "mocha",
    "coverage": "istanbul cover  _mocha -- -R spec",
    "format": "prettier-eslint --write --prettier-last \"**/*.js\""
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-minify": "^0.4.3",
    "chai": "^4.1.2",
    "eslint": "^5.0.0",
    "istanbul": "^0.4.5",
    "mocha": "^5.2.0",
    "prettier": "^1.13.5",
    "prettier-eslint": "^8.8.2",
    "prettier-eslint-cli": "^4.7.1"
  },
  "bugs": {
    "url": "https://github.com/klyrr/krokus/issues"
  },
  "author": "Tina Umlandt",
  "homepage": "https://github.com/klyrr/krokus#readme",
  "main": "index.js",
  "directories": {
    "test": "test"
  }
}
